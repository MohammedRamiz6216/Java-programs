import java.util.*;


public class Functions {
    public static void FindTheAverage(int a, int b, int c){
        int Average;
       Average = (a+b+c)/3;
         System.out.print("The Average is: " + Average);
        return;
    }
    public static void main(String args[]){
     Scanner sc = new Scanner(System.in);
     int a = sc.nextInt();
     int b = sc.nextInt();
     int c = sc.nextInt();
     FindTheAverage(a,b,c);
    }
}
